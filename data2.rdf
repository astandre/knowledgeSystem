<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:ns0="https://schema.org/"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <rdf:Description rdf:about="http://example.com/resources/Nina">
    <rdf:type rdf:resource="http://purl.org/spar/fabioPoem"/>
    <ns0:name>Nina</ns0:name>
    <ns0:overview>Es un poema largo que alude a la leyenda de quechua de Nina Yacu.</ns0:overview>
    <ns0:genre rdf:resource="http://dbpedia.org/ontologyPoem"/>
    <ns0:sdDatePublished>1847</ns0:sdDatePublished>
    <ns0:citation>Leyenda Quichua</ns0:citation>
    <ns0:autor>
      <rdf:Description rdf:about="http://example.com/resources/MiguelRiofrio">
        <owl:sameAs rdf:resource="http://dbpedia.org/page/Miguel_Riofrio"/>
        <ns0:hasOccupation rdf:resource="http://dbpedia.org/page/poet"/>
        <ns0:hasOccupation rdf:resource="http://dbpedia.org/page/lawyer"/>
        <ns0:hasOccupation rdf:resource="http://dbpedia.org/page/professor"/>
        <ns0:hasOccupation rdf:resource="http://dbpedia.org/page/writer"/>
        <ns0:birthDate>1819-09-07</ns0:birthDate>
        <ns0:birthPlace rdf:resource="http://dbpedia.org/page/Loja_Province"/>
        <ns0:deathDate>1879-10-11</ns0:deathDate>
        <ns0:deathPlace rdf:resource="http://dbpedia.org/page/Peru"/>
        <ns0:alumniOf>Pedro Cevallos</ns0:alumniOf>
      </rdf:Description>
    </ns0:autor>

    <ns0:isPartOf>
      <rdf:Description rdf:about="http://example.com/resources/SilabasGramaticales">
        <rdfs:comment>Son las sílabas resultantes de la división silábica de cada una de las palabras.</rdfs:comment>
      </rdf:Description>
    </ns0:isPartOf>

    <ns0:isPartOf>
      <rdf:Description rdf:about="http://example.com/resources/SilabasPoeticas">
        <rdfs:comment>Son las sílabas gramaticales teniendo en cuenta las sinalefas que se forman en el verso.</rdfs:comment>
      </rdf:Description>
    </ns0:isPartOf>

    <ns0:isPartOf>
      <rdf:Description rdf:about="http://example.com/resources/Rimas">
        <rdfs:comment>Se considera rima de un verso los fonemas a partir de la última vocal tónica del verso, esta incluida.</rdfs:comment>
      </rdf:Description>
    </ns0:isPartOf>

    <ns0:hasPart rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</ns0:hasPart>
  </rdf:Description>

